<h1>Mentoras#mentored</h1>
<p>Find me in app/views/mentoras/mentored.html.erb</p>

<%= form_for @mentored, url: url_for(action: 'new_mentored') do |f| %>
  <%= label_tag(:name, "Name") %>
  <%= f.text_field :name %>
  <%= label_tag(:location, "Location") %>
  <%= f.text_field :location %>
  <%= label_tag(:description, "About you") %>
  <%= f.text_area :description %>
  <%= label_tag(:email, "Email")%>
  <%= f.email_field :email %>
  <%= f.submit "Contact me!" %>
<% end %>
